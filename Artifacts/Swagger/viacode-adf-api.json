                        "location",
                        "state"
                      ]
                    }
                  ]
                }
              },
              "required": [
                "value"
              ]
            }
          }
        }
      },
      "put": {
        "operationId": "put-adfinstances",
        "summary": "AdfInstances",
        "parameters": [
          {
            "$ref": "#/parameters/SubscriptionIdParameter"
          },
          {
            "$ref": "#/parameters/ResourceGroupNameParameter"
          }
        ],
        "responses": {
          "200": {
            "description": "Empty HTTP 200 status code",
            "schema": {
            }
          }
        }
